# Environment Variables
# Copy file ini ke masing-masing service folder sebagai .env

# Database URLs (masing-masing service punya DB sendiri)
USER_DB_URL=postgresql://...user_db...
WALLET_DB_URL=postgresql://...wallet_db...
PAYMENT_DB_URL=postgresql://...payment_db...
NOTIFICATION_DB_URL=postgresql://...notification_db...
CREDIT_DB_URL=postgresql://...credit_db...

# Service URLs (isi setelah deploy ke Vercel)
WALLET_SERVICE_URL=https://wallet-service-xxx.vercel.app
NOTIFICATION_SERVICE_URL=https://notification-service-xxx.vercel.app
